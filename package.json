{
  "name": "NomadPlanet",
  "description": "A boilerplate application for express mongoose",
  "version": "1.0.0",
  "author": {
    "name": "Madhusudhan Srinivasa",
    "email": "me@madhums.me"
  },
  "engines": {
    "node": "0.10.x"
  },
  "scripts": {
    "start": "grunt && NODE_PATH=./config:./app/controllers NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "postinstall": "bower install --config.interactive=false"
  },
  "dependencies": {
    "bower": "^1.3.12",
    "cron": "^1.0.6",
    "ejs": "^2.2.3",
    "express": "^4.11.1",
    "feedparser": "^0.19.2",
    "html-to-text": "^1.2.0",
    "mongoose": "^3.8.22"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-less": "^1.0.0",
    "grunt-contrib-uglify": "^0.7.0"
  },
  "keywords": [
    "mvc",
    "boilerplate"
  ]
}
